@model int
@{
    ViewBag.Title = "Error";

    string msjerror = "";

    switch (Model)
    {
        case 1:
            msjerror = "No cuenta con una licencia valida";
            break;
        case 2:
            msjerror = "Su licencia ha expirado";
            break;
        case 3:
            msjerror = "Su licencia esta desactivada";
            break;
    }


    if (Request.IsAjaxRequest())
    {
        Response.StatusCode = 404;
        Layout = null;
    }
}
@if (Request.IsAjaxRequest())
{
    <h2 class="text-center">@msjerror</h2>
}
else
{
    <div class="alert alert-danger" role="alert">
        <h3>@msjerror</h3>
    </div>
}




