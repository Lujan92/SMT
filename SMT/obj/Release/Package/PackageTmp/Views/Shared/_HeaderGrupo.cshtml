@model SMT.Models.DB.HeaderGrupoReporte
@{
    ViewBag.Title = "_HeaderGrupo";
}

<table style="width:100%;margin:auto">
    <tr>
        <th colspan="3" class="text-center">@Model.escuela - @Model.clave</th>
    </tr>
    <tr>
        <th class="text-center">@Model.grupo @Model.grado - @Model.materia</th>
    </tr>
    <tr>
        <th class="text-center">@Model.maestro</th>
    </tr>
    <tr>
        <th class="text-center">@Model.titulo</th>
    </tr>
</table>