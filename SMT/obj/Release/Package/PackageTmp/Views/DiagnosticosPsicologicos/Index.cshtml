@model SMT.Models.DB.Grupos
@{
    ViewBag.Title = Model.Materia + " " + Model.Grado + Model.Grupo + " - Diagnóstico psicológico";

}
@section styles{
<style>
    .vertical{
        padding:1px !important;
       overflow:visible;
       height:200px;
    }

    .vertical .ajustar{
        
       width:100px ;
       margin-left:0;
    }
</style>
    }
<div>
    <h1>
        @ViewBag.Title
    </h1>
</div>


<div class="panel panel-default hidden-print">
    <div class="panel panel-heading">Acciones de Diagnóstico psicológico</div>
    <div class="panel panel-body">
        <button class="btn btn-danger visor-oculto" onclick="DiagnosticoPsicologico.Imprimir()"><i class="fa fa-print fa-fw"></i> Imprimir</button>
        <a href="/grupos/@Model.IDGrupo/Bimestre/1" class="pull-right btn btn-info">Regresar</a>
    </div>
</div>

<div class="panel panel-default">
    <div class="panel panel-heading hidden-print">Diagnóstico psicológico</div>
    <div class="panel panel-body">
        <div data-psicologico="@Model.IDGrupo" style="overflow:auto"></div>

    </div>
</div>

<div id="divInternoImpresion"></div>

@section scripts{
    <script src="~/Scripts/apps/Bimestre/Alumnos.js"></script>
    <script src="~/Scripts/apps/DiagnosticoPsicologico/DiagnosticoPsicologico.js"></script>
    <script>
        var _grupo = '@Model.IDGrupo';
        setTimeout(function () {


            DiagnosticoPsicologico.desplegarTabla("@Model.IDGrupo");

        }, 800);
    </script>
}
