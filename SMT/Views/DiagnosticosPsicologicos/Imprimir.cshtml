            <td  data-campo="RespetaAsiMismo" data-respuesta="4"><div>@(diag != null ? diag.RespetaAsiMismo:"")</div></td>
            <td  data-campo="ResuelveConfilctos" data-respuesta="4"><div>@(diag != null ? diag.ResuelveConfilctos:"")</div></td>
